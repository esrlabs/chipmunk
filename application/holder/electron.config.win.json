    },
    "asarUnpack": [
        "resources"
    ],
    "npmRebuild": true,
    "afterSign": "hooks/afterSign.js",
    "win": {
        "icon": "resources/win/chipmunk.ico",
        "target": [],
        "publisherName": "ESR Labs"
    }
}