{
    "asar": true,
    "appId": "com.esrlabs.chipmunk",
    "productName": "chipmunk",
    "copyright": "Copyright Â© 2022 year ESR Labs",
    "files": [
        "./dist/**",
        "./resources/**"
    ],
    "extraFiles": [
        {
            "from": "../apps/precompiled/updater/target/release",
            "to": "Resources/bin",
            "filter": [
                "updater",
                "updater.exe"
            ]
        }
