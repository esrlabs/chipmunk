syntax = "proto3";

package grabbed_element;

message GrabbedElement {
  uint32 source_id = 1;
  string content = 2;
  uint64 pos = 3;
  uint32 nature = 4;
}

message SdeRequest {
  oneof request {
    string write_text = 1;
    bytes write_bytes = 2;
  }
}

message SdeResponse {
  uint64 bytes = 1;
}

message AttachmentInfo {
  string uuid = 1;
  string filepath = 2;
  string name = 3;
  string ext = 4;
  uint64 size = 5;
  string mime = 6;
  repeated uint64 messages = 7;
}

message Range {
  uint64 start = 1;
  uint64 end = 2;
}

message ObserveOptions {
  ObserveOrigin origin = 1;
  ParserType parser = 2;
}

message DltParserSettings {
  DltFilterConfig filter_config = 1;
  repeated string fibex_file_paths = 2;
  bool with_storage_header = 3;
  string tz = 4;
}

message DltFilterConfig {
  uint32 min_log_level = 1;
  repeated string app_ids = 2;
  repeated string ecu_ids = 3;
  repeated string context_ids = 4;
  int64 app_id_count = 5;
  int64 context_id_count = 6;
}

message SomeIpParserSettings {
  repeated string fibex_file_paths = 1;
}

message ProcessTransportConfig {
  string cwd = 1;
  string command = 2;
  map<string, string> envs = 3;
}

message SerialTransportConfig {
  string path = 1;
  uint32 baud_rate = 2;
  uint32 data_bits = 3;
  uint32 flow_control = 4;
  uint32 parity = 5;
  uint32 stop_bits = 6;
  uint32 send_data_delay = 7;
  bool exclusive = 8;
}

message TCPTransportConfig {
  string bind_addr = 1;
}

message MulticastInfo {
  string multiaddr = 1;
  string interface = 2;
}

message UDPTransportConfig {
  string bind_addr = 1;
  repeated MulticastInfo multicast = 2;
}

message OperationDone {
  string uuid = 1;
  string result = 2;
}

message Ticks {
  uint64 count = 1;
  string state = 2;
  uint64 total = 3;
}

message Notification {
  Severity severity = 1;
  string content = 2;
  uint64 line = 3;
}

message NativeError {
  Severity severity = 1;
  NativeErrorKind kind = 2;
  string message = 3;
}

enum Severity {
  WARNING = 0;
  ERROR = 1;
}

enum NativeErrorKind {
  FileNotFound = 0;
  UnsupportedFileType = 1;
  ComputationFailed = 2;
  Configuration = 3;
  Interrupted = 4;
  OperationSearch = 5;
  NotYetImplemented = 6;
  ChannelError = 7;
  Io = 8;
  Grabber = 9;
}

message GrabError {
  oneof error {
    string config = 1;
    string communication = 2;
    string io_operation = 3;
    InvalidRange invalid_range = 4;
    bool interrupted = 5;
    bool not_initialize = 6;
    string unsupported = 7;
  }
}

message InvalidRange {
  uint64 start = 1;
  uint64 end = 2;
  string context = 3;
}

message FileFormat {
  enum Type {
    PcapNG = 0;
    PcapLegacy = 1;
    Text = 2;
    Binary = 3;
  }
}

message ParserType {
  oneof type {
    DltParserSettings dlt = 1;
    SomeIpParserSettings some_ip = 2;
    bool text = 3;
  }
}

message Transport {
  oneof transport {
    ProcessTransportConfig process = 1;
    TCPTransportConfig tcp = 2;
    UDPTransportConfig udp = 3;
    SerialTransportConfig serial = 4;
  }
}

message ObserveOrigin {
  oneof origin {
    File file = 1;
    Concat concat = 2;
    Stream stream = 3;
  }

  message File {
    string name = 1;
    FileFormat.Type format = 2;
    string path = 3;
  }

  message Concat {
    repeated File files = 1;
  }

  message Stream {
    string name = 1;
    Transport transport = 2;
  }
}

message CallbackEvent {
  oneof event {
    uint64 stream_updated = 1;
    bool file_read = 2;
    SearchUpdated search_updated = 3;
    IndexedMapUpdated indexed_map_updated = 4;
    SearchMapUpdated search_map_updated = 5;
    SearchValuesUpdated search_values_updated = 6;
    AttachmentsUpdated attachments_updated = 7;
    Progress progress = 8;
    NativeError session_error = 9;
    OperationError operation_error = 10;
    string operation_started = 11;
    string operation_processing = 12;
    OperationDone operation_done = 13;
    bool session_destroyed = 14;
  }

  message SearchUpdated {
    uint64 found = 1;
    map<string, uint64> stat = 2;
  }

  message IndexedMapUpdated {
    uint64 len = 1;
  }

  message SearchMapUpdated {
    string update = 1;
  }

  message SearchValuesUpdated {
    map<uint32, ValueRange> values = 1;

    message ValueRange {
      double min = 1;
      double max = 2;
    }
  }

  message AttachmentsUpdated {
    uint64 len = 1;
    AttachmentInfo attachment = 2;
  }

  message Progress {
    string uuid = 1;
    ProgressDetail detail = 2;

    message ProgressDetail {
      oneof detail {
        Ticks ticks = 1;
        Notification notification = 2;
        bool stopped = 3;
      }
    }
  }

  message OperationError {
    string uuid = 1;
    NativeError error = 2;
  }
}
