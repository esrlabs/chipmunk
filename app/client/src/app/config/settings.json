{
  "version": "12.03.2018",
  "drop_rules": [
    "visual"
  ],
  "visual": {
    "prevent_ascii_colors_always": false,
    "prevent_ascii_colors_on_highlight": true,
    "do_not_highlight_matches_in_requests": false,
    "highlight_search_requests": false,
    "show_active_search_results_always": true
  },
  "server":{
    "API_URL"                 : "http://127.0.0.1:8082/API",
    "WS_URL"                  : "ws://127.0.0.1:8082/",
    "WS_PROTOCOL"             : "logviewer",
    "WS_RECONNECTION_TIMEOUT" : 5000
  }
}